{
  "version": "1.0.0-next.19",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "post-install.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup-node --watch",
    "dev": "tsup-node --watch",
    "build": "shx rm -rf ./dist && tsup-node",
    "build-installer": "yarn tsc --esModuleInterop --outDir ./installer src/direct-install.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint \"src/**/*.{ts,tsx}\" && prettier  --check src/**/*.ts",
    "prepublishOnly": "yarn build",
    "typecheck": "tsc --noEmit",
    "postinstall": "node post-install.js",
    "publish:dev": "yalc publish --push"
  },
  "prettier": "@stacks/prettier-config",
  "name": "@clarigen/native-bin",
  "author": "Hank Stoever",
  "module": "dist/native-bin.esm.js",
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/node-fetch": "^2.3.4",
    "@types/unzipper": "^0.10.3"
  },
  "dependencies": {
    "@clarigen/core": "1.0.0-next.19",
    "fs-extra": "^10.0.0",
    "node-fetch": "^2.6.0",
    "unzipper": "^0.10.11"
  },
  "publishConfig": {
    "access": "public"
  }
}
